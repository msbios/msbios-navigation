{
  "name": "msbios/navigation",
  "description": "MSBios Navigation",
  "license": "GPL",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-navigation",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\Navigation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\Navigation\\": "tests/"
    }
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-navigation": "^2.8",
    "msbios/msbios": "^1.0"
  },
  "require-dev": {
    "msbios/test": "^1.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit --colors=always"
    ],
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "cache": [
      "rm -Rf data/cache/*.php",
      "rm -Rf data/cache/*.dat",
      "rm -Rf data/logs/*.log"
    ],
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  },
  "extra": {
    "zf": {
      "component": "MSBios\\Navigation"
    }
  }
}
